{
  "expo": {
    "name": "Rajasthan Bus Tracking",
    "slug": "rajasthan-bus-tracking",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/bus_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Namaste.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF8C00"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.rajasthanbustracking",
      "jsEngine": "hermes"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Namaste.png",
        "backgroundColor": "#FF8C00"
      },
      "package": "com.yourcompany.rajasthanbustracking",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "softwareKeyboardLayoutMode": "pan",
      "jsEngine": "hermes"
    },
    "web": {
      "favicon": "./assets/bus_logo.png"
    }
  }
}